    "paths": {
        "/api/auth/login": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Handle a login request to the application.",
                "description": "Login by user_name and password",
                "operationId": "login",
                "requestBody": {
                    "description": "Pass user credentials",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "user_name",
                                    "password"
                                ],
                                "properties": {
                                    "user_name": {
                                        "type": "string",
                                        "example": "gerente"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "1234"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Authenticated token",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "token": {
                                            "type": "string",
                                            "example": "eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJpc3MiOiJodHRwOi8vbG9jYWxob3N0L2FwaS9hdXRoL2xvZ2luIiwiaWF0IjoxNjM1NDU5NzM3LCJleHAiOjE2MzU0NjMzMzcsIm5iZiI6MTYzNTQ1OTczNywianRpIjoiVXozanpUeElNNnJBSDJCSiIsInN1YiI6MiwicHJ2IjoiMjNiZDVjODk0OWY2MDBhZGIzOWU3MDFjNDAwODcyZGI3YTU5NzZmNyJ9.TWsfFNrcU4dFGnbNyNbudzQSwdCknOKGrImic5BROtw"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/rooms": {
            "get": {
                "tags": [
                    "Rooms"
                ],
                "description": "Get list of all rooms",
                "operationId": "/api/rooms",
                "responses": {
                    "200": {
                        "description": "Return List of all rooms",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
        },
        "/api/api/rooms/occupied": {
            "get": {
                "tags": [
                    "Rooms"
                ],
                "description": "Get list of rooms that are not available.",
                "operationId": "/api/rooms/occupied",
                "responses": {
                    "200": {
                        "description": "Return list of rooms.",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
        },
        "/api/api/rooms/available": {
            "get": {
                "tags": [
                    "Rooms"
                ],
                "description": "Get list of rooms that are available.",
                "operationId": "/api/rooms/available",
                "responses": {
                    "200": {
                        "description": "Return list of rooms.",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
        },
        "/api/api/rooms/{id}": {
            "get": {
                "tags": [
                    "Rooms"
                ],
                "description": "Get list of rooms that are available.",
                "operationId": "/api/rooms/{id}",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Id of rooms to show",
                        "required": true,
                        "schema": {
                            "type": "int"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Rooms information.",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
        },
        "/api/api/rooms": {
            "post": {
                "tags": [
                    "Rooms"
                ],
                "description": "Save list of rooms on database",
                "operationId": "/api/rooms",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Id of rooms to show",
                        "required": true,
                        "schema": {
                            "type": "int"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Rooms information.",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "responses": {
            "403": {
                "description": "Forbidden",
                "content": {
                    "application/json": {
                        "schema": {
                            "properties": {
                                "token": {
                                    "type": "string",
                                    "example": ""
                                }
                            },
                            "type": "object"
                        }
                    }
                }
            }
        }
    }
}